{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "types/*": ["./src/@types/*"],
      "*": ["./src/*"]
    },
    "jsx": "react",
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "out",
    "**/*.snap.*"
  ]
}
