  "scripts": {
    "install-esbuild": "node ../node_modules/esbuild/install.js",
    "start:dev": "yarn install-esbuild && nodemon",
    "start": "node dist/index.js",
    "build": "tsc --build",
    "migrate:create": "ts-node -r tsconfig-paths/register migrate.ts create",
    "migrate:up": "ts-node -r tsconfig-paths/register migrate.ts up",
    "migrate:rollback": "ts-node -r tsconfig-paths/register migrate.ts rollback",
    "migrate:down": "ts-node -r tsconfig-paths/register migrate.ts rollback",
    "migrate:reset": "ts-node -r tsconfig-paths/register migrate.ts reset",
    "test": "jest",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "lint:ci": "yarn lint --quiet",
    "lint:types": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@fono/horn": "1.0.0",
    "@fono/record": "1.0.0",
